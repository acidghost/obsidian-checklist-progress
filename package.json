{
  "name": "obsidian-checklist-progress",
  "version": "0.12.0",
  "description": "Plugin to automatically fill progress (as fraction or percentage) of check-lists.",
  "main": "build/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian-md",
    "obsidian-plugin"
  ],
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@types/node": "^20.14.0",
    "builtin-modules": "^4.0.0",
    "esbuild": "^0.21.4",
    "obsidian": "latest",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
